<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>

  <display-name>Online shopping</display-name>
  <description>This is small online shopping site</description>

<servlet>
    <servlet-name>Homepage</servlet-name>
    <display-name>Homepage</display-name>
    <servlet-class>Homepage</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  
  <servlet>
    <servlet-name>Secondpage</servlet-name>
    <display-name>Secondpage</display-name>
    <servlet-class>Secondpage</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  
  <servlet>
    <servlet-name>ThirdPage</servlet-name>
    <display-name>ThirdPage</display-name>
    <servlet-class>ThirdPage</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>


  <servlet>
    <servlet-name>AdminDetails</servlet-name>
    <display-name>AdminDetails</display-name>
    <servlet-class>AdminDetails</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>LoginSucces</servlet-name>
    <display-name>LoginSucces</display-name>
    <servlet-class>LoginSucces</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>


  <servlet>
    <servlet-name>AdminLogin</servlet-name>
    <display-name>AdminLogin</display-name>
    <servlet-class>AdminLogin</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
    
    <servlet>
    <servlet-name>UploadFiles</servlet-name>
    <display-name>UploadFiles</display-name>
    <servlet-class>UploadFiles</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>

<servlet>
    <servlet-name>UploadArtist</servlet-name>
    <display-name>UploadArtist</display-name>
    <servlet-class>UploadArtist</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>  

 
  <servlet-mapping>
    <servlet-name>Homepage</servlet-name>
    <url-pattern>/Homepage</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
    <servlet-name>Secondpage</servlet-name>
    <url-pattern>/Secondpage</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ThirdPage</servlet-name>
    <url-pattern>/ThirdPage</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>AdminDetails</servlet-name>
    <url-pattern>/AdminDetails</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>LoginSucces</servlet-name>
    <url-pattern>/LoginSucces</url-pattern>
  </servlet-mapping>

<servlet-mapping>
    <servlet-name>AdminLogin</servlet-name>
    <url-pattern>/AdminLogin</url-pattern>
  </servlet-mapping>
    
    <servlet-mapping>
    <servlet-name>UploadFiles</servlet-name>
    <url-pattern>/UploadFiles</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>UploadArtist</servlet-name>
    <url-pattern>/UploadArtist</url-pattern>
  </servlet-mapping>




 <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>